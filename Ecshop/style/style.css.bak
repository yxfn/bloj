body,div,span,p,i,a,table,dl,dt,dd,ul,li,ol,img,h1,h2,h3,h4{margin:0px;padding:0px;}
img{border:0px;}
li{list-style:none;}
a{text-decoration:none;color:#000;font-size:12px;}
input{outline:none;border:none;}
body{font-family:"Microsoft YaHei";background:#fff;}
/*字体样式开始*/
@font-face {font-family: 'iconfont';
    src: url('../iconfont/iconfont.eot'); /* IE9*/
    src: url('../iconfont/iconfont.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
    url('../iconfont/iconfont.woff') format('woff'), /* chrome、firefox */
    url('../iconfont/iconfont.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
    url('../iconfont/iconfont.svg#iconfont') format('svg'); /* iOS 4.1- */
}
.iconfont{
    font-family:"iconfont" !important;
    font-size:16px;font-style:normal;
	color:#ed145b;
	}
/*字体样式结束*/
/*!--网站头部开始--*/
/*!--网站头部图片开始--*/
#TopMain {position:relative;z-index:15;}
#TopMain .Top_img{width:100%;height:100px;background:url("../images/advbg.png"); text-align:center;}
#TopMain .Top_img a.Top_a_img{display:block;width:1000px;height:100px;background:url("../images/adv.jpg");}
#TopMain .Top_img .imgm{position:absolute;right:0px;top:0px;cursor:pointer;}
/*!--网站头部图片结束--*/
/*网站头部LOGO,search,导购开始*/
#TopMain .Top_con{width:1000px;height:70px;margin:0 auto;padding-top:30px;}
#TopMain .Top_con .Top_c_logo{width:220px;height:70px;float:left;}
#TopMain .Top_con .Top_c_search{width:355px;height:70px;float:left;position:relative;margin-left:5px;}
#TopMain .Top_con .Top_c_search #search{width:312px;height:36px;position:absolute;left:0px;border:1px solid #ccc;line-height:36px;line-height:36px;text-indent:5px;}
#TopMain .Top_con #btn{width:43px;height:40px;position:absolute;right:0px;background:url("../images/ser_but.png") center right;cursor:pointer;}
/* #TopMain .Top_con .Top_c_search #search{width:312px;height:36px;border:1px solid #ccc;position:absolute;left:0px;}
#TopMain .Top_con #btn{width:41px;height:40px;position:absolute;right:0px;background:url("../images/ser_but.png") no-repeat center right;} */
#TopMain .Top_con .txt{position:absolute;top:0px;font:12px/40px "Microsoft YaHei";text-indent:5px;color:#ddd;display:inline;}
#TopMain .Top_con .txtcon{width:311px;height:148px;background:#fff;background:rgba(255,255,255,0.9);position:absolute;top:39px;left:0px;border:1px solid #ddd;display:none;}
#TopMain .Top_con .txtcon ul.txtshow li.txtmain{height:29px;line-height:29px;text-indent:5px;transition:.4s all;cursor:pointer;}
#TopMain .Top_con .txtcon ul.txtshow li.txtmain a{color:#555;}
#TopMain .Top_con .txtcon ul.txtshow li.txtmain:hover a{color:#f42760;transition:.4s all;}
#TopMain .Top_con .Top_c_user{width:374px;height:70px;float:right;color:#333;font-size:12px;}
#TopMain .Top_con .Top_c_user .Top_c_text{font:12px/10px "Microsoft YaHei";}
#TopMain .Top_con .Top_c_user .Top_c_text a{color:#ed145b;}
#TopMain .Top_con .Top_c_user a.color{color:#333;}
/*网站头部LOGO,search,导购结束*/
/*!--网站头部结束--*/
/*!--网站头部Logonav开始--*/
#Logonav{width:100%;height:50px;background:#000;}
.gd{position:fixed;top:0px;left:0px;z-index:10;}
#Logonav ul.Logo_con_n{width:1200px;height:50px;margin:0 auto;position:relative;z-index:1;}
#Logonav ul.Logo_con_n li.Logo_con_t{float:left;text-align:center;}
#Logonav ul.Logo_con_n li.Logo_con_t a.Logo_con_t_l{display:block;padding:0 30px;color:#fff;font:16px/50px "Microsoft YaHei";position:relative;z-index:2;}
/*二级menu开始*/
#Logonav ul.Logo_con_n li.Logo_con_first{width:190px;height:50px;background:#f42760;float:left;text-align:center;position:relative;}
#Logonav ul.Logo_con_n li.Logo_con_first .menu{width:190px;background:#000;position:absolute;top:50px;left:0px;z-index:5;}
#Logonav ul.Logo_con_n li.Logo_con_first .menu ol.m_con li.m_c_l{padding:5px 8px 5px 10px;border-bottom:1px dotted #666;position:relative;cursor:pointer;}
#Logonav ul.Logo_con_n li.Logo_con_first .menu ol.m_con li.not{border-bottom:none;}
#Logonav ul.Logo_con_n li.Logo_con_first .menu ol.m_con li.m_c_l h3.m_c_l_h{text-align:left;font:15px/29px "Microsoft YaHei";color:#f42760;font-weight:bold;}
#Logonav ul.Logo_con_n li.Logo_con_first .menu ol.m_con li.m_c_l p.m_c_m_txt{text-align:left;}
li.Logo_con_first .menu ol.m_con li.m_c_l p.m_c_m_txt a.m_c_main_a{font:14px/30px "Microsoft YaHei";color:#fff;font-weight:bold;margin-right:15px;}
li.Logo_con_first .menu ol.m_con li.m_c_l p.m_c_m_txt a.red{color:#f42760;}
li.Logo_con_first .menu ol.m_con li.m_c_l p.m_c_m_txt a.m_c_main_a:hover{color:#f42760;}
/*morenav更多内容开始*/
li.Logo_con_first .menu ol.m_con li.m_c_l .morenav{width:190px;height:300px;background:#fff;position:absolute;top:-1px;left:190px;display:none;box-shadow:0 1px 3px rgba(255,255,255,.5);}
li.Logo_con_first .menu ol.m_con li.m_c_l .moretop{width:190px;height:1px;background:#fff;position:absolute;top:-1px;left:0px;display:none;}
li.Logo_con_first .menu ol.m_con li.m_c_l .morebottom{width:190px;height:1px;background:#fff;position:absolute;bottom:-1px;left:0px;display:none;}
li.Logo_con_first .menu ol.m_con li.m_c_l .moreleft{width:1px;height:69px;background:#fff;position:absolute;right:-1px;top:0px;display:none;}
li.Logo_con_first .menu ol.m_con li.hover .morenav,
li.Logo_con_first .menu ol.m_con li.hover .moretop,
li.Logo_con_first .menu ol.m_con li.hover .morebottom,
li.Logo_con_first .menu ol.m_con li.hover .moreleft{display:block;}
/*morenav更多内容结束*/
/*二级menu结束*/
#Logonav ul.Logo_con_n li.Logo_con_first a.Logo_con_t_l{font:16px/50px "Microsoft YaHei";color:#fff;font-weight:bold;}
#Logonav ul.Logo_con_n li.Logo_con_first font.iconfont{color:#fff;}
#Logonav ul.Logo_con_n li.last{width:100px;height:50px;background:#f42760;position:absolute;left:190px;top:0px;display:none;}
/*!--网站头部Logonav结束--*/
